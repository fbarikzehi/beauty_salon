<div class="modal fade text-left" id="add_discount" tabindex="-1" role="dialog" aria-labelledby="myModalLabel17" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable" role="document">
        <div class="modal-content" style="background-color: #262C49;font-size: 15px !important;font-weight: bolder;font-family: 'bnazanin';">
            <div class="modal-header bg-primary white">
                <h4 class="modal-title" id="myModalLabel17">ثبت تخفیف</h4>
            </div>
            <div class="modal-body mt-75 mb-25">
                <div class="form-body">
                    <div class="row">
                        <div class="col-md-12 col-lg-12 col-sm-12 col-12">
